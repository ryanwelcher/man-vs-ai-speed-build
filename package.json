		"iapi": "npx @wordpress/create-block@latest --template @ryanwelcher/all-the-things --variant interactive"
	},
	"devDependencies": {
		"@wordpress/prettier-config": "^4.13.0"
	},
	"prettier": "@wordpress/prettier-config"
